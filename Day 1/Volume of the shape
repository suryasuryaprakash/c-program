#include <stdio.h>
#include <math.h>
#define PI 3.14159265359
int main() 
{
    int l, b, h, r;
    scanf("%d %d %d %d", &l, &b, &h, &r);
    double cuboidVolume = l * b * (double)h;
    double cylinderVolume = PI * r * r * (double)h;
    double coneVolume = (1.0/3.0) * PI * r * r * (double)h;
    double sphereVolume = (4.0/3.0) * PI * pow(r, 3);
    printf("Volume of Cuboid=%.2lf\n", cuboidVolume);
    printf("Volume of Cylinder=%.2lf\n", cylinderVolume);
    printf("Volume of Cone=%.2lf\n", coneVolume);
    printf("Volume of Sphere=%.2lf\n", sphereVolume);
    return 0;
}
