#include <stdio.h>

int main() {
    double x1, y1, x2, y2;
    scanf("%lf %lf", &x1, &y1);
    scanf("%lf %lf", &x2, &y2);
    double midpoint_x = (x1 + x2) / 2.0;
    double midpoint_y = (y1 + y2) / 2.0;
    printf("Binoy's house is located at (%.1f,%.1f)\n", midpoint_x, midpoint_y);
    return 0;
}
