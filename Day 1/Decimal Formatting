#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

     float num;
    
    scanf("%f",&num);
    printf("%.5f\n",num);
    printf("%.4f\n",num);
     printf("%.3f\n",num);
     printf("%.2f\n",num);
     printf("%.1f\n",num);
     printf("%d\n",(int)num);
    return 0;
}
