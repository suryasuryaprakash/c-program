 #include <stdio.h>
int main() {
    int n, i, evenCount = 0, oddCount = 0;
    scanf("%d", &n);
    int arr[n];
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if(arr[i] % 2 == 0)
            evenCount++;
        else
            oddCount++;
    }
    if(evenCount == n)
        printf("Even\n");
    else if(oddCount == n)
        printf("Odd\n");
    else
        printf("Mixed\n");
    return 0;
}
