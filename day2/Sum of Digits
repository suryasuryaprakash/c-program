#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    if (n >= 10 && n <= 99) {
        int sum = (n / 10) + (n % 10);
        printf("Sum of Digit %d is %03d\n", n, sum);
    } else {
        printf("Invalid Input\n");
    }
    return 0;
}
